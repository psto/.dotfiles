exec_always echo "sway config trace: starting sway at $(date)"

include $HOME/.config/sway/variables
include $HOME/.config/sway/appearance
include $HOME/.config/sway/bar
include $HOME/.config/sway/bindsym
include $HOME/.config/sway/devices
include $HOME/.config/sway/exec
include $HOME/.config/sway/for_window
include $HOME/.config/sway/modes

include /etc/sway/config.d/*

